@{
    ViewBag.Title = "Supplier";
}

<!-- Collapsable Card Example -->
<div class="card shadow mb-4">
    <!-- Card Header - Accordion -->
    <a href="#collapseFilter" class="d-block card-header py-3" data-toggle="collapse"
       role="button" aria-expanded="true" aria-controls="collapseFilter">
        <h5 class="m-0 font-weight-bold text-primary">Filtrar Búsqueda</h5>
    </a>
    <!-- Card Content - Collapse -->
    <div class="collapse show" id="collapseFilter">
        <div class="card-body">
            <!-- Form Row-->
            <div class="row gx-3 mb-3">
                <!-- Form Group (first name)-->
                <div class="col-md-4">
                    <label class="small mb-1 font-weight-bold" for="inputFirstName">Identificación:</label>
                    <input class="form-control" type="text" placeholder="Ruc del proveedor" value="" />
                </div>
                <!-- Form Group (last name)-->
                <div class="col-md-4">
                    <label class="small mb-1 font-weight-bold" for="inputLastName">Nombre:</label>
                    <input class="form-control" type="text" placeholder="Nombre comercial o razón social" value="" />
                </div>
                <!-- Action -->
                <div class="col-md-2">
                    <label class="small mb-1"></label>
                    <button class="form-control btn btn-primary" type="button">Buscar</button>
                </div>
            </div>

        </div>
    </div>
</div>

